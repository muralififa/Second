FROM ServiceNow.dbo.cmdb_ci_db_instance inst
INNER JOIN ServiceNow.dbo.cmdb_ci_environment e ON inst.u_environment = e.sys_id
WHERE  (inst.sys_class_name in ('cmdb_ci_db_mysql_instance') and inst.dv_operational_status ='ACTIVE'
 and inst.dv_support_group = 'WDBS SYBASE ON DUTY SUPPORT')  and
(inst.sys_class_name in ('cmdb_ci_db_mysql_instance') and inst.dv_operational_status ='ACTIVE'
 and inst.dv_support_group !='CBH Oracle Database') and  inst.sys_created_by not in ('srcMIDDiscovery', 'TADDM User')
  and inst.u_instance_type in ('CONTAINER - ORACLE', 'DBMS INSTANCE','READ ONLY REPLICA')
  and inst.dv_u_environment != 'Sandbox'
  and not ((inst.sys_updated_on < (getdate() -180)) and inst.dv_operational_status = 'Retired')
